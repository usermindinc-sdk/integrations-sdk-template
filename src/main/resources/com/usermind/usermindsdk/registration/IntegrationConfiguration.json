{
  "actions": {
    "fetch-metadata": {
      "description": "Sdktemplate metadata-only fetch",
      "timeout_in_seconds": 600
    },
    "fetch": {
      "description": "Sdktemplate entity fetch",
      "timeout_in_seconds": 600
    }
  },
  "dispatch": {
    "types": {
      "read": {
        "actions": {
          "fetch": {
            "kafkaTopic": "Nifi_Unified_Fetch",
            "shouldPersist": false
          },
          "fetch-metadata": {
            "kafkaTopic": "Nifi_Unified_Fetch",
            "shouldPersist": false
          }
        }
      }
    }
  },
  "driver": {
    "actionMetadataEnabled": false
  },
  "ui": {
    "authenticationEnabled": true
  },
  "onboarding": {
    "allowedFetchFrequencies": [
      "24hrs",
      "12hrs",
      "6hrs",
      "3hrs",
      "1hrs"
    ],
    "auth": {
      "helpText": "Sdktemplate",
      "keys": {
        "apiId": {
          "displayName": "API Key",
          "hidden": false
        },
        "accountName": {
          "displayName": "Account Name",
          "hidden": false
        }
      },
      "type": "keyValue"
    },
    "category": "sdktemplate",
    "disconnectable": true,
    "displayName": "Sdktemplate",
    "icons": {
      "error": "//s.usermind.com/images/channels/sdktemplate-error.svg",
      "off": "//s.usermind.com/images/channels/sdktemplate-off.svg",
      "on": "//s.usermind.com/images/channels/sdktemplate-on.svg"
    },
    "legacy": false,
    "maxConnections": 1
  }
}


